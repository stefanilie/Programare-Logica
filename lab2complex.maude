fmod COMPLEX is

protecting INT .

sort Complex .

subsort Int < Complex .

op _+i_ : Int Int -> Complex .
op _*_ : Complex Complex -> Complex [ditto] .
op _+_ : Complex Complex -> Complex [ditto] .

 vars A C  : Int .
 vars B D : Int .
 
eq (A +i B) + (C +i D) = (A + C) +i (B + D) .
eq (A +i B) * (C +i D) = ( (A * C) - (B * D) ) +i ((A * D) + (B * C) ) . 
 

 

endfm


